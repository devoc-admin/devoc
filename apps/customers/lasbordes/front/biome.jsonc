{
  "root": false,
  "extends": "//",
  "linter": {
    "rules": {
      "style": {
        "noMagicNumbers": "off",
        "useFilenamingConvention": "off"
      },
      "suspicious": {
        "useAwait": "off"
      },
      "complexity": {
        "noBannedTypes": "off",
        "useLiteralKeys": "off"
      },
      "nursery": {
        "useConsistentTypeDefinitions": "off"
      }
    }
  }
}
